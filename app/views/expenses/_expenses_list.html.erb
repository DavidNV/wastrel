<% cositas.each do |expense| %>
  <div class="row">
    <p class="col s2"><%= expense.date.strftime("%d %a") %></p>
    <p class="col s4"><%= expense.concept %></p>
    <p class="col s2"><%= expense.category.name %></p>
    <p class="col s2"><%= expense.amount %></p>
    <p class="col s2"><%= expense.transaction_type.name %></p>
  </div>
<% end %>
